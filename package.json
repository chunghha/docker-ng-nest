{
  "name": "docker-ng-nest",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint docker-ng-nest",
    "build:client-and-server-bundles": "ng build --prod && ng run docker-ng-nest:server:production",
    "build:prerender": "yarn build:client-and-server-bundles && yarn webpack:server && yarn generate:prerender",
    "build:ssr": "yarn build:client-and-server-bundles && yarn webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server -p 8080",
    "serve:ssr": "node dist/server",
    "run:ssr": "yarn build:ssr && yarn serve:ssr"
  },
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~7.1.2",
    "@angular/cdk": "~7.1.1",
    "@angular/common": "~7.1.2",
    "@angular/compiler": "~7.1.2",
    "@angular/core": "~7.1.2",
    "@angular/forms": "~7.1.2",
    "@angular/http": "~7.1.2",
    "@angular/material": "~7.1.1",
    "@angular/platform-browser": "~7.1.2",
    "@angular/platform-browser-dynamic": "~7.1.2",
    "@angular/platform-server": "~7.1.2",
    "@angular/router": "~7.1.2",
    "@nestjs/common": "^5.5.0",
    "@nestjs/core": "^5.5.0",
    "@nestjs/ng-universal": "^0.1.1",
    "@nguniversal/common": "~7.0.2",
    "@nguniversal/express-engine": "~7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "~7.0.2",
    "core-js": "^2.6.0",
    "ngx-take-until-destroy": "^5.2.3",
    "rxjs": "~6.3.3",
    "webpack-cli": "^3.1.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.2",
    "@angular/cli": "~7.1.2",
    "@angular/compiler-cli": "~7.1.2",
    "@angular/language-service": "~7.1.2",
    "@types/jasmine": "~3.3.1",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.12",
    "codelyzer": "~4.5.0",
    "express": "^4.16.4",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "reflect-metadata": "^0.1.12",
    "ts-loader": "^5.3.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typeface-lato": "^0.0.54",
    "typescript": "~3.1.6"
  }
}
