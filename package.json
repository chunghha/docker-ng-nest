{
  "name": "docker-ng-nest",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint docker-ng-nest",
    "build:client-and-server-bundles": "ng build --prod && ng run docker-ng-nest:server:production",
    "build:prerender": "yarn build:client-and-server-bundles && yarn webpack:server && yarn generate:prerender",
    "build:ssr": "yarn build:client-and-server-bundles && yarn webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server -p 8080",
    "serve:ssr": "node dist/server",
    "run:ssr": "yarn build:ssr && yarn serve:ssr"
  },
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/material": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/platform-server": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@nestjs/common": "~6.2.4",
    "@nestjs/core": "~6.2.4",
    "@nestjs/ng-universal": "^1.1.1",
    "@nestjs/platform-express": "~6.2.4",
    "@nguniversal/common": "~7.1.1",
    "@nguniversal/express-engine": "~7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "~7.1.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "core-js": "^3.1.3",
    "ngx-take-until-destroy": "^5.4.0",
    "rxjs": "~6.5.2",
    "webpack-cli": "^3.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.1",
    "@angular/cli": "8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.0.4",
    "codelyzer": "~5.0.1",
    "express": "^4.17.1",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~6.0.0",
    "reflect-metadata": "^0.1.13",
    "string-replace-loader": "^2.1.1",
    "ts-loader": "^6.0.2",
    "ts-node": "~8.2.0",
    "tslint": "~5.17.0",
    "typeface-lato": "^0.0.54",
    "typescript": "~3.4.5"
  }
}
