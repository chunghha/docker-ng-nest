{
  "name": "docker-ng-nest",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint docker-ng-nest",
    "build:client-and-server-bundles": "ng build --prod && ng run docker-ng-nest:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server -p 8080",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~6.1.9",
    "@angular/cdk": "~6.4.7",
    "@angular/common": "~6.1.9",
    "@angular/compiler": "~6.1.9",
    "@angular/core": "~6.1.9",
    "@angular/forms": "~6.1.9",
    "@angular/http": "~6.1.9",
    "@angular/material": "~6.4.7",
    "@angular/platform-browser": "~6.1.9",
    "@angular/platform-browser-dynamic": "~6.1.9",
    "@angular/platform-server": "~6.1.9",
    "@angular/router": "~6.1.9",
    "@nestjs/common": "^5.3.9",
    "@nestjs/core": "^5.3.10",
    "@nestjs/ng-universal": "^0.1.1",
    "@nguniversal/common": "~6.1.0",
    "@nguniversal/express-engine": "~6.1.0",
    "@nguniversal/module-map-ngfactory-loader": "~6.1.0",
    "core-js": "^2.5.4",
    "ngx-take-until-destroy": "^5.1.0",
    "rxjs": "~6.3.3",
    "webpack-cli": "^3.1.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.3",
    "@angular/cli": "~6.2.3",
    "@angular/compiler-cli": "^6.1.9",
    "@angular/language-service": "^6.1.9",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.11.3",
    "codelyzer": "~4.4.4",
    "express": "^4.16.3",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "protractor": "~5.4.0",
    "reflect-metadata": "^0.1.12",
    "ts-loader": "^5.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typeface-lato": "^0.0.54",
    "typescript": "~2.9.2"
  }
}
