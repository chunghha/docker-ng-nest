{
  "name": "docker-ng-nest",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint docker-ng-nest",
    "build:client-and-server-bundles": "ng build --prod && ng run docker-ng-nest:server:production",
    "build:prerender": "yarn build:client-and-server-bundles && yarn webpack:server && yarn generate:prerender",
    "build:ssr": "yarn build:client-and-server-bundles && yarn webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server -p 8080",
    "serve:ssr": "node dist/server",
    "run:ssr": "yarn build:ssr && yarn serve:ssr"
  },
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~7.2.9",
    "@angular/cdk": "~7.3.5",
    "@angular/common": "~7.2.9",
    "@angular/compiler": "~7.2.9",
    "@angular/core": "~7.2.9",
    "@angular/forms": "~7.2.9",
    "@angular/http": "~7.2.9",
    "@angular/material": "~7.3.5",
    "@angular/platform-browser": "~7.2.9",
    "@angular/platform-browser-dynamic": "~7.2.9",
    "@angular/platform-server": "~7.2.9",
    "@angular/router": "~7.2.9",
    "@nestjs/common": "^6.0.1",
    "@nestjs/core": "^6.0.1",
    "@nestjs/ng-universal": "^0.4.2",
    "@nestjs/platform-express": "~6.0.1",
    "@nguniversal/common": "~7.1.1",
    "@nguniversal/express-engine": "~7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "~7.1.1",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "core-js": "^2.6.5",
    "ngx-take-until-destroy": "^5.4.0",
    "rxjs": "~6.4.0",
    "webpack-cli": "^3.3.0",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.6",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.9",
    "@angular/language-service": "~7.2.9",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.30",
    "codelyzer": "~4.5.0",
    "express": "^4.16.4",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.2",
    "reflect-metadata": "^0.1.13",
    "ts-loader": "^5.3.3",
    "ts-node": "~8.0.3",
    "tslint": "~5.14.0",
    "typeface-lato": "^0.0.54",
    "typescript": "~3.2.4"
  }
}
